{
  "name": "r",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "webpack --watch",
    "dev": "webpack-dev-server --hot --inline",
    "flow": "flow",
    "flow:typecheck": "flow check src/components",
    "flow:coverage": "flow-coverage-report",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "pretty:q": "cd src && pretty-quick --staged",
    "format:prettier": "cd src && prettier --write '**/*.{js,jsx,css,sass,scss,json}'",
    "lint:style": "cd src && stylelint '**/*.{css,scss}'"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "8.1.1",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.21.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.6",
    "flow-bin": "^0.77.0",
    "flow-coverage-report": "^0.5.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^4.3.1",
    "jest": "^23.4.2",
    "node-sass": "^4.8.1",
    "postcss-loader": "^2.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "1.14.2",
    "pretty-quick": "^1.6.0",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.1",
    "stylelint": "^9.5.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-scss": "^3.3.0",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "dependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-map-interaction": "^1.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "jest": {
    "setupFiles": [
      "./test/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "pre-commit": [
    "pretty:q",
    "lint:style"
  ]
}
